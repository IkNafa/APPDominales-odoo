<odoo>
    <data>
        <record id="variante_bar_view_tree" model="ir.ui.view">
            <field name="name">variante.bar.view.tree</field>
            <field name="model">variante.bar</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="variante_bar_action" model="ir.actions.act_window">
            <field name="name">Tipos de barras</field>
            <field name="res_model">variante.bar</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem
            id="variante_root_menu"
            parent="config_root_menu"
            name="Variante"
            sequence="3"/>

        <menuitem
            id="variante_bar_menu"
            name="Tipos de barras"
            action="variante_bar_action"
            parent="variante_root_menu"
            sequence="1"/>

        <record id="variante_rango_view_tree" model="ir.ui.view">
            <field name="name">variante.rango.view.tree</field>
            <field name="model">variante.rango</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="variante_rango_action" model="ir.actions.act_window">
            <field name="name">Tipos de rango</field>
            <field name="res_model">variante.rango</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem
            id="variante_rango_menu"
            name="Tipos de rango"
            action="variante_rango_action"
            parent="variante_root_menu"
            sequence="2"/>

        <record id="variante_tempo_view_tree" model="ir.ui.view">
            <field name="name">variante.tempo.view.tree</field>
            <field name="model">variante.tempo</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="variante_tempo_action" model="ir.actions.act_window">
            <field name="name">Tipos de tempo</field>
            <field name="res_model">variante.tempo</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem
            id="variante_tempo_menu"
            name="Tipos de tempo"
            action="variante_tempo_action"
            parent="variante_root_menu"
            sequence="3"/>

        <record id="variante_stance_view_tree" model="ir.ui.view">
            <field name="name">variante.stance.view.tree</field>
            <field name="model">variante.stance</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="variante_stance_action" model="ir.actions.act_window">
            <field name="name">Tipos de stance</field>
            <field name="res_model">variante.stance</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem
            id="variante_stance_menu"
            name="Tipos de stance"
            action="variante_stance_action"
            parent="variante_root_menu"
            sequence="4"/>

        <record id="variante_grip_view_tree" model="ir.ui.view">
            <field name="name">variante.grip.view.tree</field>
            <field name="model">variante.grip</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="variante_grip_action" model="ir.actions.act_window">
            <field name="name">Tipos de grip</field>
            <field name="res_model">variante.grip</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem
            id="variante_grip_menu"
            name="Tipos de grip"
            action="variante_grip_action"
            parent="variante_root_menu"
            sequence="5"/>


        <record id="variante_variante_view_form" model="ir.ui.view">
            <field name="name">variante.variante.view.form</field>
            <field name="model">variante.variante</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <notebook>
                            <page name="bar" string="Bar">
                                <field name="bar_id" widget="radio" options="{'horizontal': true}"/>
                            </page>
                            <page name="rango" string="Rango">
                                <field name="rango" widget="radio" options="{'horizontal': true}"/>
                            </page>
                            <page name="tempo" string="Tempo">
                                <field name="tempo" widget="radio" options="{'horizontal': true}"/>
                            </page>
                            <page name="stance" string="Stance">
                                <field name="stance" widget="radio" options="{'horizontal': true}"/>
                            </page>
                            <page name="grip" string="Grip">
                                <field name="grip" widget="radio" options="{'horizontal': true}"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
    
</odoo>